{{>layout/header}}
  <!-- 목록 출력 영역 -->
  <div class="container marketing">
    <table class="table table-horizontal table-bordered">
      <thead class="thead-strong">
      <tr>
        <th>글 번호</th>
        <th>글 제목</th>
        <th>작성자</th>
        <th>최종 수정일</th>
      </tr>
      </thead>
      <tbody id="tbody">
      {{#posts}}
        <tr>
          <td>{{postId}}</td>
          <td><a href="/posts/update/{{postId}}">{{title}}</a></td>
          <td>{{author}}</td>
          <td>{{modifiedDate}}</td>
        </tr>
      {{/posts}}
      </tbody>
    </table>
  </div>
{{>layout/footer}}